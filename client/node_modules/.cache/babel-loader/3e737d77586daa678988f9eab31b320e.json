{"ast":null,"code":"import Auth from '@/services/Auth.js';\nexport default {\n  name: 'LoginUser',\n\n  data() {\n    return {\n      username: '',\n      password: '',\n      error: null\n    };\n  },\n\n  methods: {\n    async login() {\n      try {\n        const response = await Auth.login({\n          username: this.username,\n          password: this.password\n        });\n        this.$store.dispatch('setToken', response.data.token);\n        this.$store.dispatch('setUser', response.data.user);\n        this.$router.push({\n          name: '/'\n        });\n      } catch (error) {\n        this.error = error.response.data.error;\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuBA;AAEA;EACAA,iBADA;;EAEAC;IACA;MACAC,YADA;MAEAC,YAFA;MAGAC;IAHA;EAKA,CARA;;EAUAC;IACA;MACA;QACA;UACAH,uBADA;UAEAC;QAFA;QAIA;QACA;QACA;UAAAH;QAAA;MACA,CARA,CASA;QACA;MACA;IACA;;EAdA;AAVA","names":["name","data","username","password","error","methods"],"sourceRoot":"src/components","sources":["LoginUser.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <section class=\"container\">\r\n      <span class=\"error\" v-if=\"error\">{{error}}</span> \r\n      <div class=\"box\">  \r\n          <p>Sign in</p>\r\n\r\n          <input type=\"text\" id=\"username\" name =\"username\" class=\"btn\" \r\n          placeholder=\"Username\" v-model=\"username\" required>\r\n\r\n          <input type=\"password\" id=\"password\" name=\"password\" class=\"btn\" \r\n          placeholder=\"Password\" v-model=\"password\" required>\r\n\r\n          <input type=\"submit\" class=\"send\" name=\"Send\" value=\"Sign in\" \r\n          @click=\"login\">\r\n\r\n        <router-link to=\"register\">Register here </router-link>\r\n      </div> \r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Auth from '@/services/Auth.js'\r\n\r\nexport default {\r\n  name: 'LoginUser',\r\n  data () {\r\n    return {\r\n      username: '',\r\n      password: '',\r\n      error: null\r\n    }\